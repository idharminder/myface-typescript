<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Users</h1>
<ol class="user-list">
    <% results.forEach(function(user) {%>
        <li class="user">
            <div>
                <%- include('user_individual', {user: user}); %>
            </div>
        </li>
    <% }); %>
</ol>

<div class="bottom-nav">
    <div class="bottom-links">
        <%if (previous) { %>
            <a href="<%= previous %>">Previous</a>
        <% } %>
        <%if (next) { %>
            <a href="<%= next %>">Next</a>
        <% } %>
    </div>
</div>

</body>
<%- include('_footer') -%>
</html>

